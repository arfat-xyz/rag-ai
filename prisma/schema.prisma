generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  name     String
  password String?
}

model File {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  blobUrl      String
  userEmail    String
  openAIFileId String
  fileName     String
  createdAt    DateTime @default(now())

  @@map("files")
}

model Chatbot {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  userEmail String
  createdAt DateTime @default(now())

  @@map("chatbots")
}

